<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">

  <configuration>
   	
  
  	<!-- Alias(별명, 가명) 설정 -->
  	<typeAliases>
  		<typeAlias alias="md" type="com.springproj.biz.domain.MdVO"/>	<!-- 만들 때 나타나는 속성(type)은 무조건 써줘야 하는 속성 -->
		<typeAlias alias="brandTheme" type="com.springproj.biz.domain.BrandThemeVO"/>
  		<typeAlias alias="brandInfo" type="com.springproj.biz.domain.BrandInfoVO"/>
  		<typeAlias alias="code" type="com.springproj.biz.domain.CodeVO"/>
  		<typeAlias alias="divisionCode" type="com.springproj.biz.domain.DivisionCodeVO"/>
  		<typeAlias alias="product" type="com.springproj.biz.domain.ProductVO"/>
  		<typeAlias alias="productBuy" type="com.springproj.biz.domain.ProductBuyVO" />
  		<typeAlias alias="delivery" type="com.springproj.biz.domain.DeliveryVO" />
  		<typeAlias alias="basket" type="com.springproj.biz.domain.BasketVO" />
  		<typeAlias alias="buy" type="com.springproj.biz.domain.BuyVO" />
  		
  	</typeAliases>
  	
  	<!-- SQL Mapper 설정 -->
  	<mappers>
  		<mapper resource="mappings/mdMapping.xml"/>
  		<mapper resource="mappings/brandThemeMapping.xml"/>
  		<mapper resource="mappings/brandInfoMapping.xml"/>
  		<mapper resource="mappings/codeMapping.xml"/>
  		<mapper resource="mappings/divisionCodeMapping.xml"/>
  		<mapper resource="mappings/ProductMapping.xml"/>
  		<mapper resource="mappings/productBuyMapping.xml" />
  		<mapper resource="mappings/deliveryMapping.xml" />
  		<mapper resource="mappings/basketMapping.xml" />
  		<mapper resource="mappings/buyMapping.xml" />
  	</mappers>
  
  </configuration>