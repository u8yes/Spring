<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">

  <configuration>
   	
  
  	<!-- Alias(별명, 가명) 설정 -->
  	<typeAliases>
  		<typeAlias alias="brand" type="com.springproj.biz.domain.BrandVO"/>  	<!-- 만들 때 나타나는 속성(type)은 무조건 써줘야 하는 속성 -->
  		<typeAlias alias="md" type="com.springproj.biz.domain.MdVO"/>
		<typeAlias alias="brandTheme" type="com.springproj.biz.domain.BrandThemeVO"/>
  		<typeAlias alias="brandInfo" type="com.springproj.biz.domain.BrandInfoVO"/>
  		<typeAlias alias="brandCheck" type="com.springproj.biz.domain.BrandCheckVO"/>
  		<typeAlias alias="code" type="com.springproj.biz.domain.CodeVO"/>
  		<typeAlias alias="divisionCode" type="com.springproj.biz.domain.DivisionCodeVO"/>
  	</typeAliases>
  	
  	<!-- SQL Mapper 설정 -->
  	<mappers>
  		<mapper resource="mappings/brandMapping.xml"/>
  		<mapper resource="mappings/mdMapping.xml"/>
  		<mapper resource="mappings/brandThemeMapping.xml"/>
  		<mapper resource="mappings/brandInfoMapping.xml"/>
  		<mapper resource="mappings/brandCheckMapping.xml"/>
  		<mapper resource="mappings/codeMapping.xml"/>
  		<mapper resource="mappings/divisionCodeMapping.xml"/>
  	</mappers>
  
  </configuration>