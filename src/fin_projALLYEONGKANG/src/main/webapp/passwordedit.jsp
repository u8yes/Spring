<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>비밀번호변경</title>
</head>
<body>

	<div>
		<h1>비밀번호 변경</h1>
		<form action="" method="post">
			<table border="1">
				<tr>
					<th>새 비밀번호</th>
					<td>
						<div>
							<input type="password" id="" name=""
								placeholder="변경하실 비밀번호를 입력해주세요">
						</div>
					</td>
				</tr>
				<tr>
					<th>비밀번호 확인</th>
					<td>
						<div>
							<input type="password" id="" name=""
								placeholder="변경하실 비밀번호를 다시 입력해주세요">
						</div>
					</td>
				</tr>
				<tr>
					<td colspan="2">
						<div>
							<button type="submit">비밀번호 변경</button>
						</div>
					</td>
				</tr>
			</table>
		</form>
	</div>



</body>
</html>